{
  "license": "MIT",
  "name": "@mint-machine/erc721",
  "version": "0.1.2",
  "author": "bhbryant",
  "repository": {
    "url": "https://github.com/mintmachine-xyz/mint-machine-erc721",
    "type": "git"
  },
  "scripts": {
    "build": "yarn run clean && yarn run compile",
    "clean": "yarn hardhat clean",
    "compile": "yarn hardhat compile",
    "test": "yarn hardhat test",
    "coverage": "yarn run build && yarn hardhat coverage --temp artifacts --network coverage",
    "start:local": "hardhat node",
    "deploy:local": "yarn hardhat run --network localhost scripts/deploy.ts",
    "deploy:rinkeby": "yarn hardhat run --network rinkeby scripts/deploy.ts"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.2",
    "@nomiclabs/hardhat-etherscan": "^2.1.6",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@typechain/ethers-v5": "^7.0.1",
    "@typechain/hardhat": "^2.3.0",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^10.0.0",
    "ethereum-waffle": "^3.4.0",
    "ethers": "^5.4.6",
    "hardhat": "^2.6.4",
    "hardhat-watcher": "^2.1.1",
    "solidity-coverage": "^0.7.17",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.2.1",
    "typechain": "^5.1.2",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.3.1",
    "install": "^0.13.0"
  }
}